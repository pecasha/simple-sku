{
    "name": "simple-sku",
    "version": "1.0.0",
    "title": "simple-sku",
    "description": "用于商城商品的简易SKU模块",
    "main": "build/index.cjs",
    "module": "build/index.mjs",
    "esnext": "build/index.mjs",
    "types": "build/index.d.ts",
    "exports": {
        ".": {
            "import": "./build/index.mjs",
            "require": "./build/index.cjs",
            "types": "./build/index.d.ts"
        }
    },
    "sideEffects": false,
    "keywords": [
        "sku"
    ],
    "files": [
        "build"
    ],
    "author": "ChiHai-Chuck<chuck@chihai.me>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/chihai-chuck/simple-sku.git"
    },
    "bugs": {
        "url": "https://github.com/chihai-chuck/simple-sku/issues"
    },
    "scripts": {
        "build": "rollup -c --bundleConfigAsCjs",
        "up": "pnpm update -i --latest"
    },
    "dependencies": {
        "@goodluck/util": "^4.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/plugin-transform-runtime": "^7.21.4",
        "@babel/preset-env": "^7.21.4",
        "@babel/runtime": "^7.21.5",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "core-js": "^3.30.1",
        "rollup": "^3.20.6",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "typescript": "^5.0.4"
    }
}
